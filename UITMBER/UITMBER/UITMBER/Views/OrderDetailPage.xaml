<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="UITMBER.Views.OrderDetailPage">
    <ContentPage.Content>
        <StackLayout Spacing="20" Padding="15">
            <Label Text="Koszt:" FontSize="Medium" />
            <Label Text="{Binding Cost}" FontSize="Small"/>
            <Label Text="Typ samochodu:" FontSize="Medium" />
            <Label Text="{Binding Type}" FontSize="Small"/>
            <Label Text="Status:" FontSize="Medium" />
            <Label Text="{Binding Status}" FontSize="Small"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>